<main class="main">
  <div class="inner">
    <%# ログインしているときは以下を表示する %>
    <% if user_signed_in? %>
      <div class="greeting">
         こんにちは、
         <%= link_to "#{current_user.name}さん", user_path(current_user), style: "text-decoration: underline;"%>
      </div>
    <% end %>
    <div class="card__wrapper">
      <%= render partial: "prototypes/prototype", collection: @prototypes, as: :prototype %>
    </div>
  </div>
</main>
